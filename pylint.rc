[MASTER]
load-plugins=pylint_django

[MESSAGES CONTROL]
# Errors only.
enable=E
disable=W,C,R,I,maybe-no-member,super-on-old-class

[REPORTS]
# Don't display the complete report.
reports=no

[TYPECHECK]
# Dynamically-set members missed by the inference system.
# generated-members=index,canonical_name,display_name,is_valid,cleaned_data,objects
